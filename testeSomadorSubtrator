v0x5c74728d4a30_0 .var "temp", 16 0;
E_0x5c74728c1120 .event anyedge, v0x5c74728d4920_0, v0x5c74728d46a0_0, v0x5c74728d4780_0, v0x5c74728d4a30_0;
    .scope S_0x5c7472885620;
T_0 ;
    %wait E_0x5c74728c1120;
    %load/vec4 v0x5c74728d4920_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 1;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 1;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %pushi/vec4 0, 0, 16;
    %store/vec4 v0x5c74728d4840_0, 0, 16;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c7472885830_0, 0, 1;
    %jmp T_0.3;
T_0.0 ;
    %load/vec4 v0x5c74728d46a0_0;
    %pad/u 17;
    %load/vec4 v0x5c74728d4780_0;
    %pad/u 17;
    %add;
    %store/vec4 v0x5c74728d4a30_0, 0, 17;
    %load/vec4 v0x5c74728d4a30_0;
    %parti/s 16, 0, 2;
    %store/vec4 v0x5c74728d4840_0, 0, 16;
    %load/vec4 v0x5c74728d4a30_0;
    %parti/s 1, 16, 6;
    %store/vec4 v0x5c7472885830_0, 0, 1;
    %jmp T_0.3;
T_0.1 ;
    %load/vec4 v0x5c74728d46a0_0;
    %pad/u 17;
    %load/vec4 v0x5c74728d4780_0;
    %pad/u 17;
    %sub;
    %store/vec4 v0x5c74728d4a30_0, 0, 17;
    %load/vec4 v0x5c74728d4a30_0;
    %parti/s 16, 0, 2;
    %store/vec4 v0x5c74728d4840_0, 0, 16;
    %load/vec4 v0x5c74728d4a30_0;
    %parti/s 1, 16, 6;
    %inv;
    %store/vec4 v0x5c7472885830_0, 0, 1;
    %jmp T_0.3;
T_0.3 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5c7472885490;
T_1 ;
    %pushi/vec4 61697, 0, 16;
    %store/vec4 v0x5c74728d4c70_0, 0, 16;
    %pushi/vec4 61456, 0, 16;
    %store/vec4 v0x5c74728d4d40_0, 0, 16;

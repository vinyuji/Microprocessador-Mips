    .port_info 4 /OUTPUT 1 "Cout";
v0x5c7472885830_0 .var "Cout", 0 0;
v0x5c74728d46a0_0 .net "a", 15 0, v0x5c74728d4c70_0;  1 drivers
v0x5c74728d4780_0 .net "b", 15 0, v0x5c74728d4d40_0;  1 drivers
